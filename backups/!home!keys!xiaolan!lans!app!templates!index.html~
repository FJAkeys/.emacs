<script type="text/javascript">
var map = new BMap.Map("mapcontainer");    //地图容器

var points = [                          //创建点
    new BMap.Point(104.052003,30.55275)

];
var marker1 = new BMap.Marker(points[0]);   //创建标注

map.addOverlay(marker1);                    //显示标注


map.centerAndZoom(points[0], 18);                 // 初始化地图，设置中心点坐标和地图级别
map.addControl(new BMap.NavigationControl());      //为地图添加鱼骨

var label = new BMap.Label("小蘭精選",{position : points[0], offset: new BMap.Size(3,-6)});       //定义一个文字标签，注意1.2请用position
map.addOverlay(label);

marker1.addEventListener("click",function(){
    map.setViewport(points);
});

</script>





